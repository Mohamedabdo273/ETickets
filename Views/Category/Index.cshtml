@model IEnumerable<Category>
<table class="table table-hover">
    <thead>       
            <h1>
             Categories
            </h1>
    </thead>
    <tbody>
       <th scope="row">Name</th>
       @foreach(var category in Model)
       {
        <tr class="table-active">       
           <th>
               @category.Name
           </th>
            <th><a asp-controller="Category" asp-action="AllCategory" asp-route-id="@category.Id" >All Movies</a></th>
        </tr>
       }
    </tbody>
</table>
